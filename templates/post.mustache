{{> header}}

{{#post}}
<article>
  {{#internal}}
    {{#title}}
      <h1>{{title}}</h1>
    {{/title}}
  {{/internal}}
  {{^internal}}
  {{#htmlTitle}}{{{htmlTitle}}}{{/htmlTitle}}
  {{/internal}}
  {{{html}}}

  {{^internal}}
  <time datetime="{{created}}">
    <a href="{{{url}}}" title="{{created}}">
      {{createdDate}}
    </a>
  </time>
  {{/internal}}
</article>
{{/post}}

{{> footer}}
